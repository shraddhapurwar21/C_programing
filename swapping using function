#include<stdio.h>
void swap(int* a,int* b){
    int temp=*a;
    *a=*b;
    *b=temp;
    return ;
}
int main(){
    int a,b;
    printf("a and b is");
    scanf("%d%d",&a,&b);
    printf("value before swap a=%d,b= %d\n",a,b);
    swap(&a,&b);
    printf("value after swap a=%d,b= %d\n",a,b);
    return 0;
}
